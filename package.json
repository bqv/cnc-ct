{
  "name": "cnc",
  "version": "1.0.0",
  "description": "ClicknClear Coding Test",
  "main": "build/server/app.js",
  "scripts": {
    "lint": "npm run lint:client && npm run lint:server",
    "lint:client": "eslint --ignore-path .gitignore --ignore-pattern node_modules --fix -- .",
    "lint:server": "tslint -c tslint.json -p tsconfig.json --fix",
    "prebuild": "rimraf build && npm run lint",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack -p --progress --config=config/webpack.config.prod.js",
    "build:server": "tsc && copyfiles -a -u 1 server/**/**/* build/server",
    "prettier": "prettier --ignore-path .gitignore --write './**/*.js'",
    "start": "node .",
    "dev:start": "npm run build && npm run start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "test": "echo No tests available && exit 1"
  },
  "author": "Tony Olagbaiye",
  "license": "UNLICENSED",
